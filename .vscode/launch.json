{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Ember",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/ember",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "CMake: build",
      "miDebuggerPath": "/usr/bin/gdb"
    },
    {
      "name": "Run Tests",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/runUnitTests",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "CMake: build",
      "miDebuggerPath": "/usr/bin/gdb"
    }
  ]
}
